{
  // diagnostic-languageserver
  "diagnostic-languageserver.filetypes": {
    "vim": "vint",
    "email": "languagetool",
    "markdown": [ "write-good", "markdownlint" ],
    "vimwiki.markdown.pandoc": [ "write-good", "markdownlint" ],
    "sh": "shellcheck",
    "elixir": ["mix_credo", "mix_credo_compile"],
    "eelixir": ["mix_credo", "mix_credo_compile"],
    "php": ["phpstan", "psalm"],
  },
  "coc.preferences.enableMarkdown": true,
  "markdownlint.config": {
    "markdownlint.enable": true,
    "markdownlint.filetypes": [
       "asciidoc",
       "asciidoctor",
       "adoc",
       "asciidoc.markdown",
       "vimwiki",
       "vimwiki.markdown"
  ]
  },
  "markdownlint.filetypes": [
     "asciidoc",
     "asciidoctor",
     "adoc",
     "asciidoc.markdown",
     "vimwiki",
     "vimwiki.markdown"
  ],
